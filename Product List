  <script>
  var products = [
      {
          name:"Cheerios"
      },
      {
          name:"Campbells Chicken Noodle Soup"
      },
      {
          name:"Fig Newtons"
      },
  ];

  for(var i = 0; i < products.length; i++){
      var pName = products[i].name;
      $("#table-container").append("<div style='border:2px black solid; padding:10px; margin-bottom:1px;'><tr>"+pName+"</tr></div>");
  }

</script>
